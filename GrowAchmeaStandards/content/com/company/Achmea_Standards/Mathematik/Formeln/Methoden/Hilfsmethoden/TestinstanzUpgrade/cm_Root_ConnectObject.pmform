  <pm:sourcecode language="m10">
    <pm:text encoding="none"><![CDATA[#import "incStdMethods_BASIS.h"

//Externe Pfade
extern path pathVkprodAlt
extern path pathVkprodNeu
extern path pathProdNeu
extern path pathProdAlt
//Externe Objekte
extern object extProdAlt
extern object extRoot

//Versicherungstechnische Methoden
data int vtm_ConnectObject

//Objekte
object objAltProd	// Altes Verkaufsprodukt
object objNeuProd	// Neues Verkaufsprodukt


//interne Variablen
 
/*********************************************************************/
/* Verarbeitung                                                      */
/*********************************************************************/

interface_method = FALSE

loop ( this, pathVkprodAlt )
{	
	loop ( this, pathVkprodNeu )
	{
		loop(pathVkprodNeu, pathProdNeu )
		{
			objNeuProd = pathProdNeu
			loop(pathVkprodAlt, pathProdAlt )
			{
				objAltProd = pathProdAlt
				rc = objNeuProd.vtm_ConnectObject.exec
								[extProdAlt	= objAltProd
								,extRoot = this]
			}
		}
	}
}

Res = RC_OK

#import "incStdMethodsBottom_BASIS.h"
]]></pm:text>
